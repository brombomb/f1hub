{
  "name": "dtt",
  "dockerComposeFile": "docker-compose.yml",
  "service": "dtt",
  "workspaceFolder": "/workspace",
  "postCreateCommand": "mkdir -p node_modules && sudo chown -R node:node node_modules && npm ci",
  "remoteUser": "node",
  "settings": {},
  "extensions": [],
  "forwardPorts": [
    9000
  ],
  "portsAttributes": {
    "9000": {
      "label": "Dev Server",
      "onAutoForward": "openBrowserOnce"
    }
  }
}
